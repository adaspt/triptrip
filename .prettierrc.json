{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "none",
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^react-(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@utils/(.*)$",
    "^@model/(.*)$",
    "^@api/(.*)$",
    "^@hooks/(.*)$",
    "^@components/(.*)$",
    "^\\.\\./(.*(?<!\\.(json|s?css|svg)))$",
    "^\\./(.*(?<!\\.(json|s?css|svg)))$",
    "^([^.].*\\.s?css)$",
    "^\\.(.*)\\.s?css$",
    "^[./]"
  ]
}
